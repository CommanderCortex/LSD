package me.cortex.LSD.Tokenizer;

public class Token {
	
	private String token;
	private TokenType type;
	
	public Token(String token, TokenType type) {
		this.token = token;
		this.type = type;
		
	}
	
	public String getToken() {
		return token;
	}
	
	public Object getValue() {
		return value;
	}
	
	public TokenType getType() {
		return type;
	}
	
}
